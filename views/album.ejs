<!DOCTYPE html>
<html>

<head>
  <title>Sigma Phi Delta Beta-Nu</title>
  <link rel="icon" type="image/png" href="/favicon.ico"/>
  <% include base/styles %>
</head>

<body>
  
  <div id="navbar">
    <% include base/navbar %>
  </div>
  
  <div id="main">
    <div class="container">
      <h1><%= album[0]["album"] %></h1>
      <% for (var i = 0; i < album.length; i++) { %>
        <% var urls = album[i]["url"] %>
        <% var img = "/images" + urls["photo"] %>
        <% var link = "/gallery" + urls["album"] + urls["photo"] %>
        <span class="gallery_thumb" style="margin-right: 16px;">
          <a href=<%= link %>>
            <div>
              <img src=<%= img %> />
            </div>
            <h2><%= album[i]["title"] %></h2>
          </a>
        </span>
      <% } %>
    </div>
  </div>
  
  <% include base/scripts %>
  
</body>

</html>
