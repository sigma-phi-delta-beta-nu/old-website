<%         /* contact_us.ejs */         %>
<%       /* Contact Us partial */       %>
<%    /* Written by Brandon Kelley */   %>

<section>
  <h1>Elected Officials</h1>
  <table style="width: 100%">
    <col style="width: 35%"/>
    <col style="width: 30%"/>
    <col style="width: 30%"/>
    <% var elected = positions.elected %>
    <% for(var i = 0; i < elected.length; i++) { %>
      <tr>
        <td><%= elected[i].position %></td>
        <td><%= elected[i].name.first %> <%= elected[i].name.last %></td>
        <td><%= elected[i].email %></td>
      </tr>
    <% } %>
  </table>
</section>
<section>
  <h1>Appointed Officials</h1>
  <table style="width: 100%">
    <col style="width: 35%"/>
    <col style="width: 30%"/>
    <col style="width: 30%"/>
    <% var appointed = positions.appointed %>
    <% for(var i = 0; i < appointed.length; i++) { %>
      <tr>
        <td><%= appointed[i].position %></td>
        <td><%= appointed[i].name.first %> <%= appointed[i].name.last %></td>
        <td><%= appointed[i].email %></td>
      </tr>
    <% } %>
  </table>
</section>
